<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="false"
        android:state_focused="false">
        <shape
            android:shape="rectangle">
            <gradient
                android:angle="270"
                android:endColor="@color/color_backbutton"
                android:startColor="@color/color_backbutton"
                android:centerColor="@color/color_backbutton"/>
            <corners
                android:radius="12dp" />
            <stroke android:color="@color/color_backstroke"
                android:width="1dp"/>


        </shape>
    </item>
</selector>